{
  "manifest_version": 2,
  "name": "LINC assistant",
  "version": "1.0",
  "default_locale": "en",
  "description": "That extenshion make easy to use LINC",
  "icons": {
    "48": "Images/LINC-icon@48.png",
    "128": "LINC-icon@128.png"
  },

  "permissions": [ "tabs", "storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "*://classroom.google.com/*", "activeTab"],


  "content_scripts": [
    {
      "matches": ["http://*.classroom.google.com/*"],
      "js": ["classroom.js"]
    }
  ]

}